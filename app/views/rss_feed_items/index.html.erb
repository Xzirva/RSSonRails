<p id="notice"><%= notice %></p>

<h1>Listing Rss Feed Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rss_feed_items.each do |rss_feed_item| %>
      <tr>
        <td><%= link_to 'Show', rss_feed_item %></td>
        <td><%= link_to 'Edit', edit_rss_feed_item_path(rss_feed_item) %></td>
        <td><%= link_to 'Destroy', rss_feed_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rss feed item', new_rss_feed_item_path %>
